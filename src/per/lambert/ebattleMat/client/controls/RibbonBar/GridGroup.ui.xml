<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.doubleboxstyle {
		height: 12px;
		width: 25px;
		text-align: right;
	}
	
	.mypanel {
		background-color: #C3D9FF;
		border: 1px solid #000000;
		padding: 3px;
		margin: 3px;
		font-weight: normal;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:FlowPanel ui:field="panel">
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:CheckBox ui:field="showGrid" text="Show Grid" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Grid Size" />
					</g:customCell>
					<g:customCell>
						<g:DoubleBox ui:field="gridSizeBox" styleName="{style.doubleboxstyle}" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="OffsetX" />
					</g:customCell>
					<g:customCell>
						<g:DoubleBox ui:field="gridOffsetX" styleName="{style.doubleboxstyle}" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="OffsetY" />
					</g:customCell>
					<g:customCell>
						<g:DoubleBox ui:field="gridOffsetY" styleName="{style.doubleboxstyle}" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:FlowPanel>
