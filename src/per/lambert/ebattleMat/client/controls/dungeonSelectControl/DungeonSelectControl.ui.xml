<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.panel {
		background-color: ivory;
	}
	</ui:style>
	<g:DockLayoutPanel width="300px" height="300px">
		<g:center>
			<g:FlowPanel styleName="{style.panel}">
				<g:ListBox ui:field="dungeonDropdownList" />
				<g:Button text="Select Dungeon" ui:field="selectButton" />
				<g:CheckBox ui:field="asGM" text="Open as GM" />
				<g:Button text="Create New Dungeon" ui:field="createDungeonButton" />
			</g:FlowPanel>
		</g:center>
		<g:south size="20">
			<g:Button ui:field="accept" text="OK" />
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 