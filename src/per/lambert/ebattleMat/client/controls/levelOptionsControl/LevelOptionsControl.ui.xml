<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.panel {
		background-color: ivory;
	}
	.sessionLabel {
		width: 100%;
		height: 23px;
		margin-right: 4px;
		margin-top: 8px;
		vertical-align: middle;
		color: black;
		background: darkgrey;
		font-family: arial;
		font-size: 9pt;
		text-align: center;
		padding-top: 5px;
	}
	</ui:style>
	<g:DockLayoutPanel width="400px" height="350px" styleName="{style.panel}">
		<g:north size="30">
			<g:Label styleName="{style.sessionLabel}">Dungeon Level Options</g:Label>
		</g:north>
		<g:center>
			<g:FlowPanel>
				<g:Grid ui:field="dmGrid">
				</g:Grid>
			</g:FlowPanel>
		</g:center>
		<g:south size="30">
			<g:HorizontalPanel>
				<g:Button ui:field="ok">Ok</g:Button>
				<g:Button ui:field="cancel">Cancel</g:Button>
			</g:HorizontalPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 