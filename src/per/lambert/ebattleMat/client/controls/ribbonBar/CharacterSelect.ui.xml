<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="panel">
			<g:Button ui:field="dungeonControl" text="Dungeon Select ..."/>
			<g:ListBox ui:field="characterSelect" visibleItemCount="1"/>
			<g:ListBox ui:field="monsterSelect" visibleItemCount="1"/>
			<g:ListBox ui:field="levelSelect" visibleItemCount="1"/>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 